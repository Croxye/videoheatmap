.team-wrapper
  background: $gray

.team
  h1
    margin-top: 0

.member
  h1
    font-size(15)
    color: $blue
  h2
    font-size(13)
    font-weight: 400
    color: $dark-gray
    margin-bottom: 20px
  ul
    clearfix()
    margin: 0
    list-style: none
    padding: 0
  li
    float: left
    margin-right: 10px
  a
    text-decoration: none

.member-img
  margin-bottom: 20px
  border: 1px solid darken($gray, 10%)

